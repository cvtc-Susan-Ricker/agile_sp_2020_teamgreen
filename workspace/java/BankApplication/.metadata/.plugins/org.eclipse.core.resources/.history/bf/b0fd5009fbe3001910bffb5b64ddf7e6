package edu.cvtc.edu;

/**
 * @author david.kittle
 *
 */
public class BankAccountDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		// Create array of bank account 
		BankAccount[] accounts = new BankAccount[4];
		

		// one 
		accounts[0] = new BankAccount(); 
		accounts[0].setBalance(95.0);
		
		// two 
		accounts[1] = new BusinessCheckingAccount(99.0);
		
		// three
		accounts[2] = new FreeCheckingAccount(100.0);
		
		// four
		accounts[3] = new SavingsAccount(101.1);
		
		
		
		
		

	}

}
